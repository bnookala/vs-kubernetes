{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "script",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": 0,
        "eqeqeq": "error"
    }
}